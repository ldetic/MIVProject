@model IEnumerable<MIVProject.userType>
@using Resources
@{
    ViewBag.Title = Local.UserType;
    //Layout = "~/Views/Shared/_LayoutCMS.cshtml";
}

<h2>@Local.UserType</h2>

<p>
    @Html.ActionLink(Local.CreateNew, "Create")
</p>
<table id="userType-table" class="table object-table" data-toggle="table" data-sort-name="id" data-sort-order="asc"
       data-search="false" data-toolbar="#toolbar" data-pagination="true"
       data-query-params="queryParams">
    <thead>
        <tr>
            <th data-sortable="true">
                @Local.Type
            </th>
            <th></th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.type)
            </td>
            <td>
                @Html.ActionLink(Local.Edit, "Edit", new { id = item.typeID }) |
                @Html.ActionLink(Local.Delete, "Delete", new { id = item.typeID })
            </td>
        </tr>
    }

</table>
