@model IEnumerable<MIVProject.userType>
@using Resources
@{
    ViewBag.Title = @Local.UserType;
    //Layout = "~/Views/Shared/_LayoutCMS.cshtml";
}

<h2>@Local.UserType</h2>

<p>
    @Html.ActionLink(@Local.CreateNew, "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.type)
        </td>
        <td>
            @Html.ActionLink(Local.Edit, "Edit", new { id=item.typeID }) |            
            @Html.ActionLink(Local.Delete, "Delete", new { id=item.typeID })
        </td>
    </tr>
}

</table>
